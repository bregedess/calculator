{
    "name": "jakmall/recruitment-calculator",
    "description": "Jakmall Software Engineer Technical Test : Jakmall Calculator.",
    "keywords": ["jakmall"],
    "license": "proprietary",
    "type": "project",
    "repositories": [{
        "packagist": false
    }, {
        "type": "composer",
        "url": "https://packagist.org/"
    }],
    "require": {
        "php": ">=7.3.4",
        "illuminate/config": "5.8.*",
        "illuminate/console": "5.8.*",
        "illuminate/container": "5.8.*",
        "illuminate/database": "5.8.*",
        "illuminate/events": "5.8.*",
        "illuminate/filesystem": "5.8.*",
        "illuminate/support": "5.8.*",
        "illuminate/routing": "5.8.*",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "Jakmall\\Recruitment\\Calculator\\": "src/",
            "Jakmall\\Recruitment\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
